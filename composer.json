{
    "name": "nacosvel/rooster-server",
    "description": "Nacosvel Rooster Server.",
    "keywords": [
        "Nacosvel Rooster Server"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/database": "^9.52",
        "nacosvel/console": "^1.0",
        "nacosvel/container-interop": "^1.0",
        "nacosvel/database-manager": "^1.0",
        "nacosvel/facde": "^1.0",
        "nacosvel/rooster-contracts": "^1.0",
        "nacosvel/rooster-server-implementation": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nacosvel\\RoosterServer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "nacosvel": {
            "commands": [
                "Nacosvel\\RoosterServer\\Command\\MigrationCommand",
                "Nacosvel\\RoosterServer\\Command\\PublishCommand",
                "Nacosvel\\RoosterServer\\Command\\SeederCommand"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
